{
  "name": "sms-loc",
  "description": "SMSLocFD",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint ./src",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --skipLibCheck true",
    "fmt": "prettier --write ./src",
    "fmt.check": "prettier --check ./src",
    "ionic-serve": "yarn configure:dev -y && ionic serve",
    "ionic-build": "yarn configure:prod -y && ionic build ionic cap copy",
    "ionic-sync": "yarn server-sync && ionic cap sync",
    "ionic-capacitor-open-android": "ionic cap open android",
    "ionic-capacitor-run-android": "ionic capacitor run android",
    "ionic-live-reload": "ionic cap run android --livereload-url=http://localhost:8100",
    "ionic-capacitor-add-android": "ionic capacitor add android",
    "ionic-capacitor-add-ios": "ionic capacitor add ios",
    "ionic-capacitor-copy-android": "ionic capacitor copy android",
    "cordova-res-android": "cordova-res android --skip-config --copy",
    "cap": "cap",
    "docker:build": "./deploy/docker/docker-build.sh",
    "docker:compile": "./deploy/docker/docker-compile.sh",
    "docker:generate:keystore": "./deploy/docker/docker-generate-keystore.sh",
    "docker:interactive": "./deploy/docker/docker-interactive.sh",
    "configure:dev": "./config/configure.dev.sh",
    "configure:prod": "./config/configure.prod.sh",
    "trapeze": "trapeze",
    "server-sync": "cd server && yarn sync-server"
  },
  "dependencies": {
    "@capacitor-community/contacts": "^6.1.1",
    "@capacitor-community/sqlite": "^7.0.0",
