    "@capacitor/clipboard": "^7.0.0",
    "@capacitor/core": "^7.1.0",
    "@capacitor/device": "^7.0.0",
    "@capacitor/haptics": "^7.0.0",
    "@capacitor/keyboard": "^7.0.0",
    "@capacitor/network": "^7.0.0",
    "@capacitor/preferences": "^7.0.0",
    "@capacitor/share": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@hugotomazi/capacitor-navigation-bar": "^4.0.1",
    "@ionic/vue": "^8.5.0",
    "@ionic/vue-router": "^8.5.0",
    "@tanstack/vue-query": "^5.68.0",
    "@vueuse/core": "^13.0.0",
    "capacitor-nodejs": "https://github.com/hampoelz/capacitor-nodejs/releases/download/v1.0.0-beta.7/capacitor-nodejs.tgz",
    "colormap": "^2.3.2",
    "dayjs": "^1.11.13",
    "ionicons": "^7.4.0",
    "maplibre-contour": "^0.1.0",
    "maplibre-gl": "^5.2.0",
    "marked": "^15.0.7",
    "ol": "^10.4.0",
    "pinia": "^3.0.1",
    "pretty-bytes": "^6.1.1",
    "swiper": "^11.2.5",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.1.0",
    "@ionic/cli": "^7.2.0",
    "@trapezedev/configure": "^7.1.3",
    "@types/colormap": "^2.3.4",
    "@types/marked": "^6.0.0",
    "@vitejs/plugin-legacy": "^5.4.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.17.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "native-run": "^2.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
